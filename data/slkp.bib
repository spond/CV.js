	mesh = {Humans; Broadly Neutralizing Antibodies; HIV Antibodies; HIV Infections; Epitopes; Dermatitis; HIV-1},
	month = {Jun},
	number = {6},
	pages = {e1011416},
	pmc = {PMC10309625},
	pmid = {37384622},
	pst = {epublish},
	title = {Antigen pressure from two founder viruses induces multiple insertions at a single antibody position to generate broadly neutralizing HIV antibodies},
