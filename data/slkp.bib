	mesh = {Adult; CD4 Lymphocyte Count; Cluster Analysis; Female; Genes, pol; HIV Infections; HIV-1; Humans; Male; Molecular Epidemiology; Multivariate Analysis; Risk Factors; United States; Viral Load},
	month = {Oct},
