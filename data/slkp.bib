	pages = {239-72},
	pmid = {22399462},
	pst = {ppublish},
	title = {Evolution of viral genomes: interplay between selection, recombination, and other forces},
	volume = {856},
	year = {2012},
	bdsk-url-1 = {http://dx.doi.org/10.1007/978-1-61779-585-5_10}}

@article{Wertheim:2011lc,
	abstract = {Effective population screening of HIV and prevention of HIV transmission are only part of the global fight against AIDS. Community-level effects, for example those aimed at thwarting future transmission, are potential outcomes of treatment and may be important in stemming the epidemic. However, current clinical trial designs are incapable of detecting a reduction in future transmission due to treatment. We took advantage of the fact that HIV is an evolving pathogen whose transmission network can be reconstructed using genetic sequence information to address this shortcoming. Here, we use an HIV transmission network inferred from recently infected men who have sex with men (MSM) in San Diego, California. We developed and tested a network-based statistic for measuring treatment effects using simulated clinical trials on our inferred transmission network. We explored the statistical power of this network-based statistic against conventional efficacy measures and find that when future transmission is reduced, the potential for increased statistical power can be realized. Furthermore, our simulations demonstrate that the network statistic is able to detect community-level effects (e.g., reduction in onward transmission) of HIV treatment in a clinical trial setting. This study demonstrates the potential utility of a network-based statistical metric when investigating HIV treatment options as a method to reduce onward transmission in a clinical trial setting.},
	author = {Wertheim, Joel O and Pond, Sergei L Kosakovsky and Little, Susan J and De Gruttola, Victor},
	date-added = {2015-09-17 15:20:38 +0000},
	date-modified = {2015-09-17 15:20:38 +0000},
	doi = {10.1371/journal.pone.0027775},
	journal = {PLoS One},
	journal-full = {PloS one},
	mesh = {Clinical Trials as Topic; Computer Simulation; HIV; HIV Infections; Humans; Male},
	number = {11},
	pages = {e27775},
	pmc = {PMC3218056},
	pmid = {22114692},
	pst = {ppublish},
	title = {Using HIV transmission networks to investigate community effects in HIV prevention trials},
	volume = {6},
	year = {2011},
	bdsk-url-1 = {http://dx.doi.org/10.1371/journal.pone.0027775}}

@article{Farrer:2011rw,
	abstract = {Batrachochytrium dendrobatidis (Bd) is a globally ubiquitous fungal infection that has emerged to become a primary driver of amphibian biodiversity loss. Despite widespread effort to understand the emergence of this panzootic, the origins of the infection, its patterns of global spread, and principle mode of evolution remain largely unknown. Using comparative population genomics, we discovered three deeply diverged lineages of Bd associated with amphibians. Two of these lineages were found in multiple continents and are associated with known introductions by the amphibian trade. We found that isolates belonging to one clade, the global panzootic lineage (BdGPL) have emerged across at least five continents during the 20th century and are associated with the onset of epizootics in North America, Central America, the Caribbean, Australia, and Europe. The two newly identified divergent lineages, Cape lineage (BdCAPE) and Swiss lineage (BdCH), were found to differ in morphological traits when compared against one another and BdGPL, and we show that BdGPL is hypervirulent. BdGPL uniquely bears the hallmarks of genomic recombination, manifested as extensive intergenomic phylogenetic conflict and patchily distributed heterozygosity. We postulate that contact between previously genetically isolated allopatric populations of Bd may have allowed recombination to occur, resulting in the generation, spread, and invasion of the hypervirulent BdGPL leading to contemporary disease-driven losses in amphibian biodiversity.},
	author = {Farrer, Rhys A and Weinert, Lucy A and Bielby, Jon and Garner, Trenton W J and Balloux, Francois and Clare, Frances and Bosch, Jaime and Cunningham, Andrew A and Weldon, Che and du Preez, Louis H and Anderson, Lucy and Pond, Sergei L Kosakovsky and Shahar-Golan, Revital and Henk, Daniel A and Fisher, Matthew C},
	date-added = {2015-09-17 15:20:38 +0000},
	date-modified = {2015-09-17 15:20:38 +0000},
	doi = {10.1073/pnas.1111915108},
