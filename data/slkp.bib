	mesh = {Computational Biology; Gene Products, env; Genetic Variation; Glycosylation; HIV-1; Humans; Male; Phylogeny; RNA, Viral; Receptors, CXCR4; Selection, Genetic; Semen; Sequence Analysis, DNA},
