	journal = {J Med Virol},
	journal-full = {Journal of medical virology},
	mesh = {Adult; Blood; Cluster Analysis; Coinfection; Cytomegalovirus; Cytomegalovirus Infections; Genotype; HIV Infections; HIV-1; Humans; Male; Molecular Sequence Data; Phylogeny; RNA, Viral; Real-Time Polymerase Chain Reaction; Semen; Sequence Analysis, DNA; Viral Load},
