	doi = {10.1093/molbev/msac061},
	journal = {Mol Biol Evol},
	journal-full = {Molecular biology and evolution},
	keywords = {coevolution; epistasis; negative selection; positive selection},
	mesh = {COVID-19; Humans; Mutation; SARS-CoV-2; Spike Glycoprotein, Coronavirus},
	month = {04},
