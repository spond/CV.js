	doi = {10.3390/v16111803},
	journal = {Viruses},
	journal-full = {Viruses},
	keywords = {epidemic lineages; evolution; natural selection; negative selection; positive selection; vesicular stomatitis virus},
	mesh = {Phylogeny; Vesicular Stomatitis; United States; Genome, Viral; Epidemics; Vesicular stomatitis Indiana virus; Humans; Evolution, Molecular; Polymorphism, Single Nucleotide; Mexico; Selection, Genetic},
	month = {Nov},
