	doi = {10.1128/JVI.02827-12},
	journal = {J Virol},
	journal-full = {Journal of virology},
	mesh = {Antibodies, Neutralizing; Computational Biology; Epitopes; Female; Genotype; HIV Antibodies; HIV Envelope Protein gp120; HIV Infections; HIV-1; Humans},
	month = {May},
	number = {10},
	pages = {5372-83},
	pmc = {PMC3648150},
	pmid = {23468492},
