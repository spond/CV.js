	journal = {PLoS One},
	journal-full = {PloS one},
	mesh = {Antibodies, Monoclonal; Antibodies, Neutralizing; Antibodies, Viral; COVID-19; COVID-19 Vaccines; Databases, Factual; Humans; Immunization, Passive; Neutralization Tests; SARS-CoV-2; Spike Glycoprotein, Coronavirus},
	number = {3},
	pages = {e0261045},
	pmc = {PMC8906623},
	pmid = {35263335},
