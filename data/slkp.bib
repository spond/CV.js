	journal = {Nature},
	journal-full = {Nature},
	mesh = {COVID-19; DNA Mutational Analysis; Evolution, Molecular; Genetic Fitness; Humans; Immune Evasion; Models, Molecular; Mutation; Phylogeny; Phylogeography; SARS-CoV-2; Selection, Genetic; South Africa; Spike Glycoprotein, Coronavirus; Time Factors},
	month = {04},
	number = {7854},
	pages = {438-443},
	pmid = {33690265},
	pst = {ppublish},
	title = {Detection of a SARS-CoV-2 variant of concern in South Africa},
