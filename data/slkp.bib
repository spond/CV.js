	mesh = {Animals; Antigens, Bacterial; B-Lymphocyte Subsets; Bacterial Proteins; DNA-Binding Proteins; Gene Rearrangement, B-Lymphocyte; Immunoglobulin Variable Region; Immunoglobulin kappa-Chains; Mice; Mice, Inbred C57BL; Peptostreptococcus; Superantigens; Virulence},
	month = {Jul},
	number = {2},
