	doi = {10.1371/journal.pone.0139510},
	journal = {PLoS One},
	journal-full = {PloS one},
	number = {10},
	pages = {e0139510},
	pmc = {PMC4592012},
	pmid = {26431315},
	pst = {epublish},
	title = {Comparative Analysis of Cell-Associated HIV DNA Levels in Cerebrospinal Fluid and Peripheral Blood by Droplet Digital PCR},
	volume = {10},
	year = {2015},
	bdsk-url-1 = {http://dx.doi.org/10.1371/journal.pone.0139510}}

