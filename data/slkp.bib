	doi = {10.1093/molbev/msaf068},
	journal = {Mol Biol Evol},
	journal-full = {Molecular biology and evolution},
	keywords = {codon substitution models; codon usage bias; molecular evolution; synonymous substitutions},
	mesh = {Animals; Selection, Genetic; Models, Genetic; Drosophila; Codon Usage; Silent Mutation; Evolution, Molecular},
	month = {Apr},
	number = {4},
	pages = {msaf068},
	pmc = {PMC11979333},
	pmid = {40129111},
	pst = {ppublish},
	title = {A New Comparative Framework for Estimating Selection on Synonymous Substitutions},
	volume = {42},
	year = {2025},
	bdsk-url-1 = {https://doi.org/10.1093/molbev/msaf068}}

@article{France:2025aa,
	abstract = {Detecting and responding to clusters of rapid HIV transmission is a core HIV prevention strategy in the United States, guiding public health interventions and identifying gaps in prevention and care services. In 2016, the Centers for Disease Control and Prevention (CDC) initiated molecular cluster detection using data from 27 jurisdictions. During 2016-2023, CDC expanded sequence reporting nationwide and deployed Secure HIV-TRACE, an application supporting health department (HD) molecular cluster detection. CDC conducts molecular cluster detection quarterly; state and local HDs analyze local data monthly. HDs began routinely reporting clusters to CDC by using cluster report forms in 2020. During 2018-2023, CDC identified 404 molecular clusters of rapid HIV transmission; 325 (80%) involved multiple jurisdictions. During 2020-2023, HDs reported 298 molecular clusters to CDC; 249 were first detected by HDs. Expanding molecular cluster detection has provided a foundation for improving service delivery to networks experiencing rapid HIV transmission.},
	author = {France, Anne Marie and Hallmark, Camden J and Panneer, Nivedha and Billock, Rachael and Russell, Olivia O and Plaster, Mary and Alberti, Jessica and Nuthan, Fathima and Saduvala, Neeraja and Philpott, David and Ocfemia, M Cheryl Ba{\~n}ez and Cope, Scott and Hernandez, Angela L and Pond, Sergei L Kosakovsky and Wertheim, Joel O and Weaver, Steven and Khader, Saja and Johnson, Kevin and Oster, Alexandra M},
